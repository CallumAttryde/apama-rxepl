package com.industry.rx_epl.operators.internals;

using com.apama.exceptions.Exception;

using com.industry.rx_epl.IResolver;

/** @private */
event EmptyNeverError {
	static action emptyGenerator(IResolver resolver) {
		resolver.complete();
	}
	static action neverGenerator(IResolver resolver) {
	}
	static action errorGenerator(IResolver resolver) {
		resolver.error(Exception("A Generated Error", "GeneratedError"));
	}
}